{
    "name": "mailauth",
    "version": "2.2.0",
    "description": "Email authentication library for Node.js",
    "main": "lib/mailauth.js",
    "scripts": {
        "test": "eslint \"lib/**/*.js\" \"test/**/*.js\" && mocha --recursive \"./test/**/*.js\" --reporter spec",
        "prepublish": "npm run man || true",
        "man": "cd man && marked-man --version `node -e \"console.log('v'+require('../package.json').version)\"` --manual 'Mailauth Help' --section 1 man.md > mailauth.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andris9/mailauth.git"
    },
    "keywords": [
        "rfc822",
        "email",
        "dkim",
        "spf",
        "arc",
        "dmarc",
        "bimi",
        "mta-sts"
    ],
    "author": "Andris Reinman",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/andris9/mailauth/issues"
    },
    "homepage": "https://github.com/andris9/mailauth",
    "devDependencies": {
        "chai": "4.3.4",
        "eslint": "7.32.0",
        "eslint-config-nodemailer": "1.2.0",
        "eslint-config-prettier": "8.3.0",
        "js-yaml": "4.1.0",
        "marked": "0.7.0",
        "marked-man": "0.7.0",
        "mbox-reader": "1.1.5",
        "mocha": "9.1.0"
    },
    "dependencies": {
        "@fidm/x509": "1.2.1",
        "ipaddr.js": "2.0.1",
        "joi": "17.4.2",
        "libmime": "5.0.0",
        "node-forge": "0.10.0",
        "nodemailer": "6.6.3",
        "psl": "1.8.0",
        "punycode": "2.1.1",
        "yargs": "17.1.1"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "bin": {
        "mailauth": "bin/mailauth.js"
    },
    "directories": {
        "man": "man"
    }
}
